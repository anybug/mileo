<!DOCTYPE html>
<html lang="fr-FR" dir="ltr">

  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <!-- ===============================================-->
    <!--    Document Title-->
    <!-- ===============================================-->
    {% set pageDescription = "Application de calcul d'indemnités kilométriques simple, rapide et opensource"|trans %}
    <title>
      {% block title %}
        {% if pageTitle is defined %}
          {{ pageTitle }}
        {% else %}
          {{ pageDescription }}
        {% endif %}
      {% endblock %} - Mileo</title>

    <meta name="msapplication-TileImage" content="assets/img/favicons/mstile-150x150.png">
    <meta name="description" content="{{ pageDescription }}">
    <meta name="theme-color" content="#ffffff">
    {% block stylesheets %}
        <!-- ===============================================-->
        <!--    Favicons-->
        <!-- ===============================================-->
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('assets/img/favicons/apple-touch-icon.png') }}">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('assets/img/favicons/favicon-32x32.png') }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('assets/img/favicons/favicon-16x16.png') }}">
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('assets/img/favicons/favicon.ico') }}">
        <link rel="manifest" href="{{ asset('assets/img/favicons/manifest.json') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/passtrength.css') }}">


        <!-- ===============================================-->
        <!--    Stylesheets-->
        <!-- ===============================================-->
        <link href="{{ asset('assets/css/theme.css') }}" rel="stylesheet" />
        <link href="{{ asset('assets/css/front.css') }}" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400&amp;display=swap" rel="stylesheet">
    {% endblock %}
    
    {%  block javascripts_head %}
        <!-- ===============================================-->
        <!--    JavaScripts-->
        <!-- ===============================================-->
        <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
        <script src="{{ asset('vendors/@popperjs/popper.min.js') }}"></script>
        <script src="{{ asset('vendors/bootstrap/bootstrap.min.js') }}"></script>
        <script src="{{ asset('vendors/is/is.min.js') }}"></script>
        <script src="https://polyfill.io/v3/polyfill.min.js?features=window.scroll"></script>
        <script src="{{ asset('assets/js/theme.js') }}"></script>
        {{ encore_entry_script_tags('newapp') }}
        <script src="{{ asset('assets/js/jquery.passtrength.js') }}"></script>
    {% endblock %}

  </head>


  <body>

    <!-- ===============================================-->
    <!--    Header Content-->
    <!-- ===============================================-->
    <header class="main" id="top">
      <nav class="navbar navbar-expand-lg navbar-light fixed-top py-3" data-navbar-on-scroll="data-navbar-on-scroll" style="background-image: none;">
        <div class="container"><a class="navbar-brand d-flex align-items-center fw-bold fs-2" href="{{ path('home') }}">
            <div class="text-primary">Mil</div>
            <div class="text-1000">eo</div>
          </a>
          <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
          <div class="collapse navbar-collapse border-top border-lg-0 mt-4 mt-lg-0" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto pt-2 pt-lg-0 align-items-center">
              <li class="nav-item"><a class="nav-link fw-medium" href="{{ path('home') }}#pointsforts">Nos points forts</a></li>
              <li class="nav-item"><a class="nav-link fw-medium" href="{{ path('home') }}#fonctionalites">Fonctionnalités</a></li>
              <li class="nav-item"><a class="nav-link fw-medium" href="{{ path('home') }}#tarifs">Tarifs</a></li>
              <li class="nav-item"><a class="nav-link fw-medium" href="{{ path('home') }}#faq">FAQ</a></li>
              <li class="nav-item"><a class="nav-link fw-medium" href="{{ path('contact') }}">Contact</a></li>
              <li class="nav-item d-flex align-items-center ps-2">
                  <img src="{{ asset('assets/img/icons/github.png')}}" alt="" height="20" width="20">
                  <a class="nav-link px-1 fw-medium" href="https://github.com/anybug/mileo" target="_blank">Github</a>
              </li>
            </ul>
            <div class="ps-lg-5">
              <a class="btn btn-lg btn-outline-primary rounded-pill" href="{{ path('security_login') }}">Connexion</a>
              <a class="btn btn-lg btn-primary rounded-pill order-0" href="{{ path('security_registration') }}">Essai gratuit</a>
            </div>
          </div>
        </div>
      </nav>
    </header>
    <!-- ============================================-->
    <!-- Main content ============================-->
    <main>
      {% block page_content %}{% endblock %}
    </main>
    <!-- ============================================-->
    <!-- Footer content ============================-->
      <footer class="py-8 bg-1000">

        <div class="container">
          <div class="row flex-center">
            <div class="col-auto mb-5"><a class="pe-2 d-flex align-items-center text-decoration-none fw-bold fs-3" href="{{ path('home') }}">
                <div class="text-primary">Mil</div>
                <div class="text-white">eo</div>
              </a></div>
          </div>
          <div class="row flex-center">
            <div class="col-auto mb-5">
              <ul class="list-unstyled list-inline mb-0">
                <li class="list-inline-item me3 me-sm-4"><a class="text-light text-decoration-none" href="{{ path('home') }}#pointsforts">Nos points forts</a></li>
                <li class="list-inline-item me3 me-sm-4"><a class="text-light text-decoration-none" href="{{ path('home') }}#fonctionalites">Fonctionnalités</a></li>
                <li class="list-inline-item me3 me-sm-4"><a class="text-light text-decoration-none" href="{{ path('home') }}#tarifs">Tarifs</a></li>
                <li class="list-inline-item me3 me-sm-4"><a class="text-light text-decoration-none" href="{{ path('home') }}#faq">FAQ</a></li>
                <li class="list-inline-item me3 me-sm-4"><a class="text-light text-decoration-none" href="{{ path('contact') }}">{{ 'Contact'|trans }}</a></li>
                <li class="list-inline-item me3 me-sm-4">
                  <img src="{{ asset('assets/img/icons/github-white.png')}}" alt="" height="20" width="20">
                  <a class="text-light text-decoration-none" href="https://github.com/anybug/mileo" target="_blank">Github</a>
                </li>
              </ul>
            </div>
          </div>
          <div class="row flex-center">

          </div>

        </div>
        <!-- end of .container-->

      </footer>
      <!-- <section> close ============================-->
      <!-- ============================================-->
      
</body>

</html>